{
  "compilerOptions": {
  "target": "ES6",
  "module": "ESNext",
  "outDir": "dist",
  "strict": true,
  "esModuleInterop": true,
  "skipLibCheck": true,
  "forceConsistentCasingInFileNames": true,
  "moduleResolution": "node",
},
  "include": ["src/**/*", "dist", "index.ts", "src/routes.ts", "src/configExpress.ts"],
  "exclude": ["node_modules"]
}
